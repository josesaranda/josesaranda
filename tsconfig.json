{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "jsx": "react",
    "baseUrl": ".",
    "strict": true,
    "importsNotUsedAsValues": "error",
    "paths": {
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@pages/*": ["src/layouts/*"],
      "@theming/*": ["src/theming/*"],
      "@services/*": ["src/services/*"]
    }
  }
}
