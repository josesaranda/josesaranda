{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "jsx": "react",
    "baseUrl": ".",
    "strict": true,
    "importsNotUsedAsValues": "error",
    "paths": {
      "@layouts/*": ["src/layouts/*"],
      "@pages/*": ["src/layouts/*"],
      "@theming/*": ["src/theming/*"],
      "@services/*": ["src/services/*"],
      "@habitatcss/*": ["libs/habitatcss/src/*"],
      "@josesaranda/*": ["libs/josesaranda/src/*"]
    }
  }
}
